{
  "expo": {
    "name": "pethub",
    "slug": "pethub",
    "scheme": "pethub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/outfit/Outfit_100Thin.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_200ExtraLight.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_300Light.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_400Regular.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_500Medium.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_700Bold.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_800ExtraBold.ttf",
            "node_modules/@expo-google-fonts/outfit/Outfit_900Black.ttf"
          ]
        }
      ]
    ]
  }
}
